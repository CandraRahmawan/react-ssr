{
  "name": "React-SSR",
  "version": "1.0.0",
  "description": "React Server Side Rendering",
  "main": "index.js",
  "author": "Candra Rahmawan",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prettier": "prettier --single-quote --trailing-comma es5 --write src/**/*.js",
    "build": "webpack",
    "start": "nodemon --require babel-core/register ./src/server --watch ./src/server",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage=false"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.1",
    "prettier": "^1.13.7",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}
