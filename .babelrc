{
  "presets": [
    "react",
    "env",
    "stage-1"
  ],
  "plugins": [
    [
      "css-modules-transform",
      {
        "extractCss": "./public/styles.css"
      }
    ],
    [
      "babel-plugin-inline-import",
      {
        "extensions": [
          ".txt"
        ]
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "markup"
        ],
        "theme": "tomorrow",
        "css": true
      }
    ]
  ]
}